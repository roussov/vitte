{
  "name": "vitte",
  "displayName": "Vitte",
  "description": "Syntaxe, snippets, format & LSP pour Vitte",
  "version": "0.1.0",
  "publisher": "vitte",
  "engines": {
    "vscode": "^1.84.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:vitte",
    "onCommand:vitte.format",
    "onCommand:vitte.startLsp"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vitte",
        "aliases": [
          "Vitte",
          "vitte"
        ],
        "extensions": [
          ".vitte",
          ".vit"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vitte",
        "scopeName": "source.vitte",
        "path": "./syntaxes/vitte.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "vitte",
        "path": "./snippets/vitte.code-snippets"
      }
    ],
    "commands": [
      {
        "command": "vitte.format",
        "title": "Vitte: Format Document"
      },
      {
        "command": "vitte.startLsp",
        "title": "Vitte: Start LSP Server"
      }
    ]
  },
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -w -p ./"
  },
  "devDependencies": {
    "@types/node": "^20",
    "typescript": "^5.4.0",
    "vscode": "^1.1.42",
    "vscode-languageclient": "^9.0.1"
  },
  "dependencies": {}
}