//! acl — listes de contrôle d'accès
struct Acl { __raw: __intrinsics::Acl }
export fn new() -> Acl { Acl{ __raw: __intrinsics::acl_new() } }
export fn allow(mut a: Acl, subject: String, action: String, resource: String) { __intrinsics::acl_allow(a.__raw, subject, action, resource) }
export fn check(a: Acl, subject: String, action: String, resource: String) -> bool { __intrinsics::acl_check(a.__raw, subject, action, resource) }
