//! tls â€” contexte TLS client
struct Tls { __raw: __ffi::Tls }
export fn connect(host: String, port: i32) -> Tls { __ffi::tls_connect(host, port) }
export fn write(mut t: Tls, b: Bytes) -> i32 { __ffi::tls_write(t.__raw, b) }
export fn read(mut t: Tls, max: i32) -> Bytes { __ffi::tls_read(t.__raw, max) }
