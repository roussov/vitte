//! mqtt â€” client MQTT
struct Mqtt { __raw: __ffi::Mqtt }
export fn connect(url: String, client_id: String) -> Mqtt { __ffi::mqtt_connect(url, client_id) }
export fn publish(mut c: Mqtt, topic: String, payload: Bytes) { __ffi::mqtt_publish(c.__raw, topic, payload) }
export fn subscribe(mut c: Mqtt, topic: String) { __ffi::mqtt_subscribe(c.__raw, topic) }
