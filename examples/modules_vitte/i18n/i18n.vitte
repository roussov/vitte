module std.i18n

pub use std.i18n.locale as locale
pub use std.i18n.plural as plural
pub use std.i18n.number as number
pub use std.i18n.datetime as datetime
pub use std.i18n.relative_time as relative_time
pub use std.i18n.message as message
pub use std.i18n.catalog as catalog
pub use std.i18n.loader_json as loader_json

pub fn best_locale(accept: &str, supported: &Vec<locale::Locale>) -> locale::Locale {
    locale::negotiate(accept, supported)
}
