//! rope — chaîne immuable en arbre
struct Rope { __raw: __intrinsics::Rope }
export fn from(s: String) -> Rope { Rope{ __raw: __intrinsics::rope_from(s) } }
export fn concat(a: Rope, b: Rope) -> Rope { __intrinsics::rope_concat(a.__raw, b.__raw) }
export fn to_string(r: Rope) -> String { __intrinsics::rope_to_string(r.__raw) }
