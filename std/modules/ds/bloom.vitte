//! bloom â€” filtre probabiliste
struct Bloom { __raw: __intrinsics::Bloom }
export fn new(bits: i32, hashes: i32) -> Bloom { Bloom{ __raw: __intrinsics::bloom_new(bits, hashes) } }
export fn add(mut b: Bloom, s: String) { __intrinsics::bloom_add(b.__raw, s) }
export fn might_contain(b: Bloom, s: String) -> bool { __intrinsics::bloom_test(b.__raw, s) }
