struct DateTime { unix_ms: i64, offset_min: i32 }
export fn now() -> DateTime { DateTime{ unix_ms: __intrinsics::now_unix_ms(), offset_min: 0 } }
export fn format(dt: DateTime, fmt: String) -> String { __intrinsics::datetime_format(dt.unix_ms, fmt) }
export fn parse(s: String, fmt: String) -> DateTime { __intrinsics::datetime_parse(s, fmt) }
