//! kv — magasin clé/valeur
struct Kv { __raw: __intrinsics::Kv }
export fn open(path: String) -> Kv { Kv{ __raw: __intrinsics::kv_open(path) } }
export fn get(kv: Kv, k: String) -> Option<Bytes> { __intrinsics::kv_get(kv.__raw, k) }
export fn set(mut kv: Kv, k: String, v: Bytes) { __intrinsics::kv_set(kv.__raw, k, v) }
