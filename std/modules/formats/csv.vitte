//! csv — parse simple (séparateur ,)
export fn parse(input: String) -> Vec<Vec<String>> {
  let rows: Vec<Vec<String>> = Vec::new();
  let lines = __intrinsics::str_split(input, "\n");
  for line in lines {
    if len(line)==0 { continue; }
    let cols = __intrinsics::csv_split(line);
    push(rows, cols);
  }
  rows
}
